    "version": "0.2.0",
    "configurations": [


        {
            "name": "Debug Android",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "android"
        },
        {
            "name": "Debug iOS",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/start"
        },
        {
            "name": "Developer's iPhone",
            "program": "${workspaceRoot}/.vscode/src/HomeScreen.js",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "target": "device",
            "sourceMaps": true,
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "CC's iPhone Xs",
            "program": "${workspaceRoot}/.vscode/src/HomeScreen.js",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "target": "device",
            "sourceMaps": true,
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "mani's iPhone",
            "program": "${workspaceRoot}/.vscode/src/HomeScreen.js",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "target": "device",
            "sourceMaps": true,
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "iPhone",
            "program": "${workspaceRoot}/.vscode/src/HomeScreen.js",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "target": "device",
            "sourceMaps": true,
            "cwd": "${workspaceRoot}"
        },
    ]
}